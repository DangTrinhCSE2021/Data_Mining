                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    -- Fact Table Measures
    SOD.SalesOrderID,
    SOD.ProductID, -- Use ProductID from SalesOrderDetail (one column)
    SOD.SpecialOfferID,
    SOH.OrderDate, -- Keep OrderDate as a single field
    SOD.UnitPrice AS SellingPrice,
    SOD.OrderQty AS SellQty,
    SO.DiscountPct AS Discount,

    -- Additional Fact Table Measures
    PRI.Quantity AS InventoryQty, -- StockedQty from PurchaseOrderDetail
    P.StandardCost AS Cost, -- UnitPrice as Cost from PurchaseOrderDetail

    -- Dimension Table Columns
    P.Name AS ProductName, -- For Product Dimension
    DP.Subcategory AS SubcategoryName, -- For Product Dimension
    DP.Category AS CategoryName, -- For Product Dimension

    
    -- Geography Dimension Columns
    A.City,
    SP.Name AS StateProvince,
    CR.Name AS CountryRegion,

    -- Promotion Dimension Columns
    SO.Description AS Promotion,
    SO.Type AS PromotionType,
    SO.DiscountPct AS PromotionDiscount,

    -- Store Dimension Columns
    ST.Name AS StoreName, -- Store Name from the Store table
    ST.BusinessEntityID, -- BusinessEntityID from the Store table

    -- Address Type Dimension Columns
    AT.Name AS AddressType, -- Name from AddressType as AddressType

    -- SalesOrderDetail ModifiedDate
    SOD.ModifiedDate -- ModifiedDate from SalesOrderDetail

FROM  [CompanyX].[Sales].[SalesOrderDetail] SOD

LEFT JOIN 
     [CompanyX].[Production].[Product] P ON SOD.ProductID = P.ProductID
LEFT JOIN 
    [CompanyX].[Sales].[SalesOrderHeader] SOH ON SOD.SalesOrderID = SOH.SalesOrderID
LEFT JOIN  
    [CompanyX].[Person].[Address] A ON SOH.ShipToAddressID = A.AddressID
LEFT JOIN  
    [CompanyX].[Sales].[SpecialOffer] SO ON SOD.SpecialOfferID = SO.SpecialOfferID
LEFT JOIN 
    [CompanyX].[dbo].[DimProduct] DP ON DP.ProductID = SOD.ProductID
LEFT JOIN  
    [CompanyX].[Person].[StateProvince] SP ON A.StateProvinceID = SP.StateProvinceID
LEFT JOIN  
    [CompanyX].[Person].[CountryRegion] CR ON SP.CountryRegionCode = CR.CountryRegionCode
LEFT JOIN  
    [CompanyX].[Purchasing].[PurchaseOrderDetail] POD ON SOD.ProductID = POD.ProductID
LEFT JOIN 
    [CompanyX].[Sales].[Customer] C ON SOH.CustomerID = C.CustomerID
LEFT JOIN 
    [CompanyX].[Sales].[Store] ST ON C.StoreID = ST.BusinessEntityID
LEFT JOIN
    [CompanyX].[Person].[BusinessEntityAddress] BEA ON A.AddressID = BEA.AddressID
LEFT JOIN   
    [CompanyX].[Person].[AddressType] AT ON BEA.AddressTypeID = AT.AddressTypeID
LEFT JOIN 
	[CompanyX].[Production].[ProductInventory] PRI ON P.ProductID = PRI.ProductID</property>
